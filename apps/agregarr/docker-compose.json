{
  "schemaVersion": 2,
  "services": [
    {
      "name": "agregarr",
      "image": "agregarr/agregarr:latest",
      "isMain": true,
      "internalPort": "7171",
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/app/config",
          "readOnly": false
        }
      ],
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ]
    }
  ]
}